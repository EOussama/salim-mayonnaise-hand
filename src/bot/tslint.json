{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"no-console": [
			false,
			"log",
			"error"
		],
		"indent": [
			true,
			"tabs",
			2
		],
		"quotemark": [
			true,
			"single"
		],
		"interface-over-type-literal": false
	},
	"rulesDirectory": []
}
